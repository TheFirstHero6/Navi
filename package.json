{
  "name": "navi",
  "version": "1.0.0",
  "description": "Navi - Your AI Fairy Helper. A powerful desktop assistant inspired by the fairy from Ocarina of Time.",
  "type": "commonjs",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:electron": "tsc -p tsconfig.node.json && npm run build",
    "preview": "vite preview",
    "electron:dev": "tsc -p tsconfig.node.json && concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "check-backend": "node check-backend.js",
    "electron:build": "npm run build:electron && electron-builder",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "author": "Klaus Chamberlain",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^35.7.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^6.0.0",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.navi.app",
    "productName": "Navi",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "backend/**/*",
      "src/assets/logo.png"
    ],
    "extraResources": [
      {
        "from": "backend",
        "to": "backend",
        "filter": [
          "**/*",
          "!node_modules/.cache/**/*"
        ]
      },
      {
        "from": "src/assets/logo.png",
        "to": "src/assets/logo.png"
      }
    ],
    "asar": false,
    "win": {
      "target": "nsis",
      "icon": "src/assets/logo.png"
    }
  }
}
